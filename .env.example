# --- GITHUB OAUTH DEV ---
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
GITHUB_CALLBACK_URL=http://localhost:8080/api/oauth/callback/github

# --- GITHUB OAUTH PROD ---
GITHUB_CLIENT_ID_PROD=your_github_client_id_prod
GITHUB_CLIENT_SECRET_PROD=your_github_client_secret_prod
GITHUB_CALLBACK_URL_PROD=https://yourdomain.com/api/oauth/callback/github

# --- GOOGLE OAUTH DEV ---

GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_CALLBACK_URL=http://localhost:8080/api/oauth/callback/google
GOOGLE_CLIENT_ID_DEV=your_google_client_id_dev
GOOGLE_CLIENT_SECRET_DEV=your_google_client_secret_dev
