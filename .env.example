# --- GOOGLE OAUTH PROD ---

GOOGLE_CLIENT_ID_PROD=your_google_client_id_prod
GOOGLE_CLIENT_SECRET_PROD=your_google_client_secret_prod
GOOGLE_CALLBACK_URL_PROD=https://yourdomain.com/api/oauth/callback/google

# --- FRONTEND ---

FRONTEND_URL=http://localhost:3000
FRONTEND_URL_DEV=http://localhost:8080
FRONTEND_URL_PROD=https://yourdomain.com

